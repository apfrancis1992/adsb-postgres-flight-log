[Unit]
Description=ADS-B Data Capture
After=network.target
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=always
RestartSec=1
User=username
ExecStart=/usr/bin/python3 /opt/adsb-postgres-flight-log/adsb.py

[Install]
WantedBy=multi-user.target
